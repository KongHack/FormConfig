{% extends '@form_config_ION/field_wrapper.twig' %}
{% block headerItem %}
    <ion-list-header id="{{ id }}">
        <ion-label><strong>
                {% if value is defined %}
                    {{ value|raw }}
                {% elseif label is defined %}
                    {{ label|raw }}
                {% endif %}
            </strong></ion-label>
    </ion-list-header>
{% endblock %}