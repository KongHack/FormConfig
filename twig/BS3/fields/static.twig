{% macro render(field) %}
    <div class="form-control-static {{ field.class }}" id="{{ field.id }}" {{ field.getDataAttributeString()|raw }}>{{ field.value|raw }}</div>
    {% if field.getScript() is not empty %}
        <script type="text/javascript">{{ field.getScript()|raw }}</script>
    {% endif %}
{% endmacro %}